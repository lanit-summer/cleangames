    <!-- From: file:/home/leontyev/Dropbox/work/apitech/ulogin-android/android-studio/SDK-dev/sdk/src/main/res/values/styles.xml -->
    <eat-comment/>
    <style name="ulogin_GenericProgressBackground" parent="android:Theme">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@android:color/white</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="ulogin_GenericProgressIndicator" parent="@android:style/Widget.ProgressBar.Small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:indeterminate">true</item>
    </style>
    <style name="ulogin_icon">
        <item name="android:layout_width">@dimen/ulogin_provider_icon_size</item>
        <item name="android:layout_height">@dimen/ulogin_provider_icon_size</item>
        <item name="android:layout_marginLeft">9dp</item>
        <item name="android:layout_marginTop">12dp</item>
        <item name="android:src">@drawable/ulogin_logo</item>
    </style>
    <style name="ulogin_item_selector">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/ulogin_item_height</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:src">@drawable/ulogin_selector</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
    </style>
    <style name="ulogin_item_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/ulogin_text_left_margin_wide_list</item>
        <item name="android:layout_toRightOf">@id/icon</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:maxLines">1</item>
    </style>
    <style name="ulogin_providers" parent="@android:style/Widget.ListView.White">
        <!--<item name="android:cacheColorHint">@color/ulogin_provider_background</item>-->
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:divider">@color/ulogin_provider_background</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
    </style>
    <style name="ulogin_theme" parent="android:Theme.Light.NoTitleBar">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:listViewStyle">@style/ulogin_providers</item>
        <!--<item name="android:background">@color/ulogin_provider_background</item>-->
    </style>
